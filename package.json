{
  "name": "webpack-start-html",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/inasekin/webpack-start"
  },
  "author": "nasekinid",
  "license": "MIT",
  "homepage": "https://github.com/inasekin/webpack-start",
  "dependencies": {
    "bash": "^0.0.1",
    "inputmask": "^5.0.7",
    "lodash": "^4.17.21"
  },
  "scripts": {
    "start": "webpack serve --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test": "eslint ./src",
    "please": "rm -rf node_modules package-lock.json dist",
    "rebuild": "rm -rf dist && npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "@commitlint/cli": "^17.3.0",
    "autoprefixer": "^10.4.13",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.0",
    "cz-customizable": "^5.3.0",
    "eslint": "^8.24.0",
    "eslint-config-htmlacademy": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.0",
    "husky": "^1.1.4",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss-loader": "^7.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "2.7.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "standard": "^16.0.4",
    "style-loader": "^3.3.1",
    "swiper": "^8.4.2",
    "terser-webpack-plugin": "^5.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "pre-commit": [
    "test"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './commitlint.js'"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "commitizen.js"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 4 version"
  ]
}
